package com.example.demo.sec.service;

import java.util.List;
import java.util.Optional;

import com.example.demo.sec.domain.Login;

public class LoginService {
    public long count();

    public void deleteById(long id);

    public void deleteAll(Iterable<Login> entities);

    public Optional<Login> findById(long id);

    public List<Login> findAllById(Iterable<Login> ids);

    public Optional<Login> findOne(Login Login);

    public List<Login> findAll();

    public Login save(Login entity);

    public List<Login> saveAll(Iterable<Login> entities);

    public boolean existsById(long id);

    public Login getOne(long id);
}
